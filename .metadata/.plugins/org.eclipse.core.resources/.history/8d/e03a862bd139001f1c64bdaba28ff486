package service.copy;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import model.Agripreneur;
import repository.AgripreneurRepository;

@Service
public class AgripreneurService {
	
	@Autowired
	private AgripreneurRepository repository;
	
	

	public  Agripreneur registerAgripreneur(Agripreneur agripreneur)
	{
		
		return repository.save(agripreneur);
	}
	
	     

}
