package model;

import java.util.Set;

import com.agri.model.Agripreneur;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.ManyToMany;

@Entity
public class Service {
	@Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long serviceId;

    private String serviceName;

    @ManyToMany(mappedBy = "services")
    private Set<Agripreneur> agripreneurs;

	public Long getServiceId() {
		return serviceId;
	}

	public void setServiceId(Long serviceId) {
		this.serviceId = serviceId;
	}

	public String getServiceName() {
		return serviceName;
	}

	public void setServiceName(String serviceName) {
		this.serviceName = serviceName;
	}

	public Set<Agripreneur> getAgripreneurs() {
		return agripreneurs;
	}

	public void setAgripreneurs(Set<Agripreneur> agripreneurs) {
		this.agripreneurs = agripreneurs;
	}

	@Override
	public String toString() {
		return "Service [serviceId=" + serviceId + ", serviceName=" + serviceName + ", agripreneurs=" + agripreneurs
				+ "]";
	}


}
